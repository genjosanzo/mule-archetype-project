<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions name="SynergyService"
	targetNamespace="http://www.tangoe.com/services/SynergyService"
	xmlns:tns="http://www.tangoe.com/services/SynergyService" xmlns:types="http://www.tangoe.com/services/SynergyService/Types"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/">

	<wsdl:types>
		<xs:schema>
			<xs:import namespace="http://www.tangoe.com/services/SynergyService/Types"
				schemaLocation="SynergyServiceTypes.xsd" />
		</xs:schema>
	</wsdl:types>

	<wsdl:message name="Invoke">
		<wsdl:part name="parameter" element="types:Invoke" />
	</wsdl:message>
	<wsdl:message name="InvokeResponse">
		<wsdl:part name="parameter" element="types:InvokeResponse" />
	</wsdl:message>
  <wsdl:message name="Dispatch">
    <wsdl:part name="parameter" element="types:Dispatch" />
  </wsdl:message>
  <wsdl:message name="DispatchResponse">
    <wsdl:part name="parameter" element="types:DispatchResponse" />
  </wsdl:message>
	<wsdl:message name="SynergyActionFault">
		<wsdl:part name="synergyActionFault" element="types:SynergyActionFault" />
	</wsdl:message>
	<wsdl:message name="SynergyServiceFault">
		<wsdl:part name="synergyServiceFault" element="types:SynergyServiceFault" />
	</wsdl:message>
	<wsdl:message name="SynergyServiceRetryableFault">
		<wsdl:part name="synergyServiceRetryableFault" element="types:SynergyServiceRetryableFault" />
	</wsdl:message>

	<wsdl:portType name="ISynergyService">
		<wsdl:operation name="Invoke">
			<wsdl:input message="tns:Invoke" />
			<wsdl:output message="tns:InvokeResponse" />
			<wsdl:fault name="synergyActionFault" message="tns:SynergyActionFault" />
			<wsdl:fault name="synergyServiceFault" message="tns:SynergyServiceFault" />
			<wsdl:fault name="synergyServiceRetryableFault" message="tns:SynergyServiceRetryableFault" />
		</wsdl:operation>
<!--  		
    <wsdl:operation name="Dispatch">
      <wsdl:input message="tns:Dispatch" />
      <wsdl:output message="tns:DispatchResponse" />
      <wsdl:fault name="synergyActionFault" message="tns:SynergyActionFault" />
      <wsdl:fault name="synergyServiceFault" message="tns:SynergyServiceFault" />
      <wsdl:fault name="synergyServiceRetryableFault" message="tns:SynergyServiceRetryableFault" />
    </wsdl:operation>
 --> 
	</wsdl:portType>

	<wsdl:binding name="ISynergyServiceServiceSoapBinding"
		type="tns:ISynergyService">
		<soap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http" />
		<wsdl:operation name="Invoke">
			<soap:operation soapAction="Invoke" style="document" />
			<wsdl:input>
				<soap:body use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" />
			</wsdl:output>
			<wsdl:fault name="synergyActionFault">
				<soap:fault name="synergyActionFault" use="literal" />
			</wsdl:fault>
			<wsdl:fault name="synergyServiceFault">
				<soap:fault name="synergyServiceFault" use="literal" />
			</wsdl:fault>
			<wsdl:fault name="synergyServiceRetryableFault">
				<soap:fault name="synergyServiceRetryableFault" use="literal" />
			</wsdl:fault>
		</wsdl:operation>
 <!--  
    <wsdl:operation name="Dispatch">
      <soap:operation soapAction="Dispatch" style="document" />
      <wsdl:input>
        <soap:body use="literal" />
      </wsdl:input>
      <wsdl:output>
        <soap:body use="literal" />
      </wsdl:output>
      <wsdl:fault name="synergyActionFault">
        <soap:fault name="synergyActionFault" use="literal" />
      </wsdl:fault>
      <wsdl:fault name="synergyServiceFault">
        <soap:fault name="synergyServiceFault" use="literal" />
      </wsdl:fault>
      <wsdl:fault name="synergyServiceRetryableFault">
        <soap:fault name="synergyServiceRetryableFault" use="literal" />
      </wsdl:fault>
    </wsdl:operation>
-->
	</wsdl:binding>

	<wsdl:service name="SynergyService">
		<wsdl:port name="ISynergyServicePort" binding="tns:ISynergyServiceServiceSoapBinding">
			<soap:address location="http://localhost:9090/ISynergyServicePort" />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
